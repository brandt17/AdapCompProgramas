import java.util.Scanner;

public class ejer6 {

    public static void main(String[] args) {
        int numero;
        int cont = 0;
        Scanner s = new Scanner(System.in);
        System.out.println("Ingrese un numero: ");
        numero = s.nextInt();
        if (numero == 0) {
            System.out.println("El 0 es una indeterminaci√≥n");
        } else {
            for (int i = 1; i <= numero; i++) {
                if (numero % i == 0) {
                    cont++;
                }
            }
            System.out.println("La contidad de divisores es: ");
            System.out.println(cont);
        }
    }
}
